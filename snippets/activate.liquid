  <section class="order-detail-section">
    <div class="container">

      {% comment %}order_info_screen{% endcomment %}
      <div class="frame8890 order_info_screen">
        <div class="div">
          {% render 'nomad-logo' %}
          <div class="div-2">
            <div class="text-wrapper">Order Information</div>
            <div class="div-wrapper">
              <p class="p">Input your Email and Order number.</p>
            </div>
          </div>
        </div>
        <div class="div-3">
          <div class="div-4">
            <div class="div-5">
              <div class="div-6">
                <div class="text-wrapper-2">Email Address</div>
              </div>
              <input
                type="email"
                class="div-wrapper-2 order_email"
                placeholder="Enter your email" />
              <div class="div-6">
                <svg
                  class="alert-circle"
                  xmlns="http://www.w3.org/2000/svg"
                  width="15"
                  height="16"
                  viewBox="0 0 15 16"
                  fill="none">
                  <path
                    d="M7.5 5.54297V8.04297M7.5 10.543H7.50625M13.75 8.04297C13.75 11.4947 10.9518 14.293 7.5 14.293C4.04822 14.293 1.25 11.4947 1.25 8.04297C1.25 4.59119 4.04822 1.79297 7.5 1.79297C10.9518 1.79297 13.75 4.59119 13.75 8.04297Z"
