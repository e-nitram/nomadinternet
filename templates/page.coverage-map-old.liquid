      var inputStr = e.target.value.replace(/\D/g,'');
      var size = inputStr.length;
      if(size == 0){
        inputStr = inputStr;
      }else if(size < 4){
        inputStr = '('+inputStr;
      }else if(size < 7){
        inputStr = '('+inputStr.substring(0,3)+') '+inputStr.substring(3,6);
      }else{
        inputStr = '('+inputStr.substring(0,3)+') '+inputStr.substring(3,6)+'-'+inputStr.substring(6,10);
      }

      $(this).val(inputStr);
    }, 100);
  } else {
    e.preventDefault();
  }
});

