				$('#product-form-pink .fee del').css('text-decoration','none');
				$('.card-pink .fee .hide_on_sim').hide();
			}
			else{
				$('.card-pink .fee .hide_on_sim').show();
				$productPrice.html(Shopify.formatMoney(variant.price, moneyFormat));
