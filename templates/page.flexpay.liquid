      $j('#screen_2').show();
      $j('#screen_3').hide();
      $j('#screen_4').hide();
      $j('.ordernumber').text(order_number);
      $j('.customer_name').text(response.data[0].customer.first_name);
      if(response.data[0].line_items.length == 1){
        order_product = response.data[0].line_items[0].name;
      }
      else if(response.data[0].line_items.length == 2){
        order_product = response.data[0].line_items[1].name;
      }
      else if(response.data[0].line_items.length == 3){
        order_product = response.data[0].line_items[2].name;
      }
       else if(response.data[0].line_items.length == 4){
        order_product = response.data[0].line_items[3].name;
      }  
      else{
        order_product = '';
      }
      console.log(order_product);
    }
  }
  else{
    $j('.rr_loader').hide();
    $j('.order_not_found_err').show();
    $j('#screen_1').show();
    $j('#screen_2').hide();
    $j('#screen_3').hide();
    $j('#screen_4').hide();
  }
  
});
  
}  

  
