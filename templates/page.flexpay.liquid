      }
      if (response.data[0].is_flexpay5 == true || response.data[0].line_items[1].variant_id == '40375589634106' || response.data[0].line_items[1].variant_id == '40375594418234' || response.data[0].line_items[1].variant_id == '40375598448698'){
        order_type = 'flex';
        $j('.id_verification_section').removeClass('disable');
