                  $j('.choose_plan_btn').addClass('already_done');
                  $j('.plan_status_pending').hide();
                  $j('.plan_status_done').show();
                  $j('.frame-5 .div-7').removeClass('active');  
                    $j('.frame-5 .div-2').addClass('disable');
                 }  
        
      } 
      else{
        $j('.id_button_main').removeClass('already_done');
        $j('.id_status_done').hide();
        if (!$('.id_verification_section').hasClass('disable')) {
          realid_check_id = response.data[0].realid_check_id;
          $j('.id_status_pending').show();
          $j('.id_verification_section').next('.component .frame-5').slideDown();
          $j('.id_verification_section').addClass('already_down');
          $j('.plan_section').next('.component .frame-5').slideUp();
          $j('.plan_section').removeClass('already_down');
        }

        if(response.data[0].zoho_subscribed == true){
            $j('.choose_plan_btn').addClass('already_done');
            $j('.plan_status_pending').hide();
            $j('.plan_status_done').show();  
            $j('.frame-5 .div-7').removeClass('active');    
          $j('.frame-5 .div-2').addClass('disable');
           }
  
