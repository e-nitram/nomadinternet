        $j('.plan_section').next('.component .frame-5').slideDown();
        $j('.plan_section').addClass('already_down');
        $j('.id_status_cleared').show();
        $j('.id_status_pending').hide();
        $j('.id_status_done').hide();
      }
      
      if(response.data[0].realid_verified == true && response.data[0].realid_status == "Done"){
        $j('.id_button_main').addClass('already_done');
        $j('.id_status_pending').hide();
