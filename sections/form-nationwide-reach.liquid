{{ '02plan-plan-v2-desktop.css' | asset_url | stylesheet_tag }}
{{ '02plan-plan-v2-mobile.css' | asset_url | stylesheet_tag }}


{% render '02-plan-coverage-popup' %}
{% render '02-plan-success-popup' %}

<span class="hidden-popup-overlay"></span>
<div class="form_nationwide " style="max-width: 100%;">
  <div class="verify-popup" js-verify-popup>
    <div class="container">
      <div class="form_input">
          <h1>Nationwide Reach</h1>
          <div class="form_title">
            <svg class="form_title_icon" width="16" height="19" viewBox="0 0 16 19" fill="none" xmlns="http://www.w3.org/2000/svg">
              <path d="M8.00001 18.0425C8.83334 13.8759 14.6667 13.3911 14.6667 8.04252C14.6667 4.36062 11.6819 1.37585 8.00001 1.37585C4.31811 1.37585 1.33334 4.36062 1.33334 8.04252C1.33334 13.3911 7.16668 13.8759 8.00001 18.0425Z" stroke="#008061" stroke-width="1.375" stroke-linecap="round" stroke-linejoin="round"/>
              <path d="M8.00001 10.5425C9.38072 10.5425 10.5 9.42323 10.5 8.04252C10.5 6.66181 9.38072 5.54252 8.00001 5.54252C6.6193 5.54252 5.50001 6.66181 5.50001 8.04252C5.50001 9.42323 6.6193 10.5425 8.00001 10.5425Z" stroke="#008061" stroke-width="1.375" stroke-linecap="round" stroke-linejoin="round"/>
            </svg>
          <h3>Where do you plan on using our service?</h3>           
          </div>
          <input
            type="text"
            placeholder="Enter Address"
            class="addr-input"
            id="autocomplete" />
          <span class="error-msg" style="display: none; color: #ff0000;">Please enter address</span>
          <button class="btn btn-primary" js-verify-address>Proceed</button>
        <p>By proceeding, you agree to receive communication from Nomad Internet.</p>
      </div>
    </div>
  </div>
</div>
{% render 'page-coverage-script' %}


{% schema %}
  {
    "name": "Nation Wide Review",
    "settings": [],
    "presets": [
      {
        "name": "Nation Wide Review"
      }
    ]
  }
{% endschema %}